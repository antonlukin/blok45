{
  "version": 3,
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "settings": {
    "layout": {
      "contentSize": "682px",
      "wideSize": "682px"
    },
    "custom": {
      "color-primary": "#1d218c",
      "color-secondary": "#eeff01",
      "color-background": "#efeded",
      "color-additional": "#FEF4E4",
      "color-accent": "#ff5741",
      "color-quote": "#f6f5f1",
      "color-disclaimer": "#8C8C8C",
      "color-dark": "#000",
      "color-light": "#fff",
      "color-border": "#d8dae2",
      "color-meta": "#111",
      "color-transparent": "#ffff"
    },
    "color": {
      "custom": false,
      "customGradient": false,
      "defaultGradients": false,
      "link": false,
      "defaultPalette": false,
      "duotone": [],
      "text": false
    },
    "typography": {
      "fontFamilies": [
        {
          "name": "Booklet Serif",
          "slug": "content",
          "fontFamily": "Booklet Serif",
          "fontFace": [
            {
              "fontFamily": "Booklet Serif",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/booklet-serif/booklet-serif-regular.woff2"
              ]
            },
            {
              "fontFamily": "Booklet Serif",
              "fontWeight": "400",
              "fontStyle": "italic",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/booklet-serif/booklet-serif-italic.woff2"
              ]
            }
          ]
        },
        {
          "name": "Struve",
          "slug": "regular",
          "fontFamily": "Struve",
          "fontFace": [
            {
              "fontFamily": "Struve",
              "fontWeight": "300",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/struve/struve-light.woff2"
              ]
            },
            {
              "fontFamily": "Struve",
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/struve/struve-regular.woff2"
              ]
            },
            {
              "fontFamily": "Struve",
              "fontWeight": "500",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/struve/struve-medium.woff2"
              ]
            },
            {
              "fontFamily": "Struve",
              "fontWeight": "600",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/struve/struve-semibold.woff2"
              ]
            },
            {
              "fontFamily": "Struve",
              "fontWeight": "700",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "file:./assets/fonts/struve/struve-bold.woff2"
              ]
            }
          ]
        }
      ],
      "defaultFontSizes": false,
      "customFontSize": false,
      "fontStyle": false,
      "dropCap": false,
      "fluid": false,
      "letterSpacing": false,
      "fontWeight": false
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": false
        }
      },
      "core/buttons": {
      },
      "core/paragraph": {
        "typography": {
          "fontFamilies": []
        },
        "color": {
          "palette": []
        }
      },
      "core/heading": {
        "typography": {
          "fontSizes": [
            {
              "slug": "small",
              "size": "1rem",
              "name": "S"
            },
            {
              "slug": "medium",
              "size": "1.5rem",
              "name": "M"
            },
            {
              "slug": "large",
              "size": "2rem",
              "name": "L"
            },
            {
              "slug": "x-large",
              "size": "2.25rem",
              "name": "XL"
            },
            {
              "slug": "xx-large",
              "size": "4rem",
              "name": "XXL"
            }
          ]
        }
      }
    },
    "spacing": {
      "units": [
        "px",
        "em",
        "rem"
      ]
    }
  },
  "styles": {
    "typography": {
      "fontSize": "1.25rem",
      "lineHeight": "1.5",
      "fontFamily": "var(--wp--preset--font-family--content)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--regular)",
          "fontSize": "2.25rem",
          "lineHeight": "1.125"
        },
        "spacing": {
          "margin": {
            "top": "3em",
            "bottom": "1em"
          }
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--regular)",
          "fontSize": "2rem",
          "lineHeight": "1.125"
        },
        "spacing": {
          "margin": {
            "top": "3em",
            "bottom": "1em"
          }
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--regular)",
          "fontSize": "1.75rem",
          "lineHeight": "1.25"
        },
        "spacing": {
          "margin": {
            "top": "1.5em",
            "bottom": "1em"
          }
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--regular)",
          "fontSize": "1.5rem",
          "lineHeight": "1.1625"
        },
        "spacing": {
          "margin": {
            "top": "1.5rem",
            "bottom": "1em"
          }
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--regular)",
          "fontSize": "1rem",
          "lineHeight": "1.375"
        },
        "spacing": {
          "margin": {
            "top": "1.25rem",
            "bottom": "1em"
          }
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--regular)",
          "fontSize": "0.875rem",
          "lineHeight": "1.5"
        },
        "spacing": {
          "margin": {
            "top": "1rem",
            "bottom": "auto"
          }
        }
      },
      "caption": {
        "color": {
          "text": "var(--wp--custom--color-meta)"
        },
        "spacing": {
          "padding": {
            "top": "0.5rem"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--regular)",
          "fontSize": "0.875rem"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--custom--color-primary)"
        },
        "border": {
          "bottom": {
            "width": "0.0625rem",
            "color": "var(--wp--custom--color-primary)",
            "style": "solid"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--regular)",
          "fontWeight": "700",
          "textDecoration": "none"
        }
      }
    },
    "blocks": {
      "core/button": {
        "color": {
          "background": "var(--wp--custom--color-accent)",
          "text": "var(--wp--custom--color-light)"
        },
        "border": {
          "radius": "0.25rem"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--regular)",
          "fontSize": "1.5rem",
          "fontWeight": "600",
          "lineHeight": "1.125"
        },
        "spacing": {
          "padding": {
            "top": "0.75rem",
            "bottom": "0.75rem",
            "left": "1rem",
            "right": "1rem"
          }
        },
        "variations": {
          "outline": {
            "color": {
              "background": "var(--wp--custom--color-light)",
              "text": "var(--wp--custom--color-accent)"
            },
            "spacing": {
              "padding": {
                "top": "0.625rem",
                "bottom": "0.625rem",
                "left": "0.875rem",
                "right": "0.875rem"
              }
            }
          },
          "secondary": {
            "color": {
              "background": "var(--wp--custom--color-additional)",
              "text": "var(--wp--custom--color-dark)"
            },
            "typography": {
              "fontSize": "1.25rem"
            },
            "spacing": {
              "padding": {
                "top": "0.5rem",
                "bottom": "0.5rem",
                "left": "0.625rem",
                "right": "0.625rem"
              }
            }
          }
        }
      },
      "core/buttons": {
        "spacing": {
          "margin": {
            "top": "1rem"
          }
        }
      },
      "core/details": {
        "color": {
          "background": "var(--wp--custom--color-background)"
        },
        "spacing": {
          "padding": {
            "top": "1rem",
            "bottom": "1rem",
            "left": "1rem",
            "right": "1rem"
          }
        },
        "typography": {
          "fontSize": "1.25rem",
          "lineHeight": "1.5",
          "fontFamily": "var(--wp--preset--font-family--regular)"
        },
        "border": {
          "radius": "0.25rem"
        },
        "css": "& summary{display: flex; align-items: center}; & summary::after{display: block; margin-left: 1rem; content:'â†“'}"
      },
      "core/post-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--regular)"
        },
        "css": "& {width: calc(100% - 2rem);}"
      },
      "core/post-content": {
        "spacing": {
          "padding": {
            "left": "1rem",
            "right": "1rem"
          }
        }
      },
      "core/list-item": {
        "spacing": {
          "margin": {
            "bottom": "0.5rem"
          }
        }
      },
      "core/separator": {
        "color": {
          "background": "var(--wp--custom--color-border)"
        },
        "border": {
          "top": {
            "width": "0.0625rem"
          }
        },
        "variations": {
          "dots": {
            "color": {
              "text": "var(--wp--custom--color-dark)"
            },
            "typography": {
              "textAlign": "left"
            },
            "spacing": {
              "margin": {
                "top": "2rem",
                "bottom": "2rem"
              },
              "padding": {
                "left": "0"
              }
            },
            "css": "&::before {content: '***'; letter-spacing: 0.5em; padding-left: 0}"
          }
        }
      },
      "core/image": {
        "border": {
          "radius": "0.25rem"
        },
        "elements": {
          "caption": {
            "css": "& {max-width: 682px; width: 100%; justify-self: center}"
          }
        }
      },
      "core/embed": {
        "elements": {
          "caption": {
            "typography": {
              "textAlign": "left"
            }
          }
        }
      }
    }
  }
}