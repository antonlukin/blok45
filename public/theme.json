{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "layout": {
      "contentSize": "960px",
      "wideSize": "1040px",
      "allowEditing": false
    },
    "typography": {
      "fontFamilies": [
        {
          "slug": "inter-variable",
          "name": "Inter Variable",
          "fontFamily": "InterVariable, sans-serif",
          "fontFace": [
            {
              "fontFamily": "InterVariable",
              "src": ["file:./assets/fonts/InterVariable.woff2"],
              "fontWeight": "100 900",
              "fontStyle": "normal"
            },
            {
              "fontFamily": "InterVariable",
              "src": ["file:./assets/fonts/InterVariable.woff2"],
              "fontWeight": "100 900",
              "fontStyle": "italic"
            }
          ]
        }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var:preset|font-family|inter-variable",
      "lineHeight": "1.65"
    },
    "spacing": {
      "padding": {
        "left": "16px",
        "right": "16px"
      }
    }
  }
}
